<idea-plugin>

    <extensions defaultExtensionNs="com.intellij">
        <localInspection
                implementationClass="com.github.suusan2go.kotlinfillclass.inspections.k2.FillDummyValuesInspection"
                groupPath="Kotlin"
                groupName="Fill class"
                enabledByDefault="true"
                level="INFORMATION"
                language="kotlin"
                displayName="Fill with dummy values"
                shortName="FillDummyValues"/>

        <localInspection
                implementationClass="com.github.suusan2go.kotlinfillclass.inspections.k2.FillVariablesOfSameNameInspection"
                groupPath="Kotlin"
                groupName="Fill class"
                enabledByDefault="true"
                level="INFORMATION"
                language="kotlin"
                displayName="Fill class constructor with variables of the same name"
                shortName="FillVariablesOfSameName"/>

        <localInspection
                implementationClass="com.github.suusan2go.kotlinfillclass.inspections.k2.FillEmptyValuesInspection"
                groupPath="Kotlin"
                groupName="Fill class"
                enabledByDefault="true"
                level="INFORMATION"
                language="kotlin"
                displayName="Fill with empty values"
                shortName="FillEmptyValues"/>
    </extensions>
</idea-plugin>